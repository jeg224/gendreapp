<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <CheckBoxPreference
        android:defaultValue="false"
        android:key="example_checkbox"
        android:summary="@string/pref_description_social_recommendations"
        android:title="@string/pref_title_social_recommendations" />

    <ListPreference
        android:defaultValue="2"
        android:entries="@array/pref_example_list_titles"
        android:entryValues="@array/pref_example_list_values"
        android:key="example_list"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_add_friends_to_messages" />
    
	<EditTextPreference 
	    android:defaultValue="Ms."
        android:key="custom_f"
	    android:title="@string/pref_title_custom_f"
	    android:enabled="false" 
	    />    
	    
	<EditTextPreference 
	    android:defaultValue="Mr."
        android:key="custom_m"
	    android:enabled="false" 
	    android:title="@string/pref_title_custom_m" 
	    />
	    
	<EditTextPreference 
	    android:defaultValue="M."
        android:key="custom_u"
	    android:enabled="false" 
	    android:title="@string/pref_title_custom_u" 
	    />    

	<CheckBoxPreference
        android:defaultValue="true"
        android:key="read_facebook"
        android:summary="@string/pref_description_read_facebook"
        android:title="@string/pref_title_read_facebook" />
			
</PreferenceScreen>
